{
  "name": "@kbhole/webx-core",
  "version": "1.0.0-alpha.1",
  "description": "WebX Protocol - Serverless web protocol where URLs carry complete page blueprints",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "webx",
    "protocol",
    "serverless",
    "decentralized",
    "url-encoding",
    "blueprint",
    "compression",
    "base62"
  ],
  "author": "WebX Protocol Foundation",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DenniiBoiii/WebXProtocol.git"
  },
  "homepage": "https://webxnexus.com",
  "bugs": {
    "url": "https://github.com/DenniiBoiii/WebXProtocol/issues"
  },
  "dependencies": {
    "pako": "^2.1.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pako": "^2.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
